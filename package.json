{
  "name": "g-test-runner",
  "version": "0.9.0",
  "description": "A simple javascript test runner (for browser)",
  "author": "Gery Debongnie",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --write src/**/*.js README.md examples/*.html",
    "build": "rollup src/setup.js --file gtest.js --format iife",
    "dev": "rollup src/setup.js --file gtest.js --format iife --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ged-odoo/g-test-runner.git"
  },
  "bugs": {
    "url": "https://github.com/ged-odoo/g-test-runner/issues"
  },
  "homepage": "https://github.com/ged-odoo/g-test-runner#readme",
  "prettier": {
    "printWidth": 100
  },
  "devDependencies": {
    "rollup": "^2.50.6"
  }
}
